{
  "name": "platonos",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "version": "1.0.0",
  "main": "server/entry.js",
  "browser": "client/entry.js",
  "author": "jirat & wirawit",
  "license": "ISC",
  "description": "",
  "eslintConfig": {
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "rules": {
      "meteor/eventmap-params": [
        2,
        {
          "templateInstanceParamName": "instance"
        }
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "draft-js": "^0.7.0",
    "eslint": "^2.13.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.13.0",
    "react": "^15.2.0",
    "react-addons-create-fragment": "^15.2.0",
    "react-addons-css-transition-group": "^15.2.0",
    "react-addons-linked-state-mixin": "^15.2.0",
    "react-addons-perf": "^15.2.0",
    "react-addons-pure-render-mixin": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-addons-transition-group": "^15.2.0",
    "react-addons-update": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-onclickoutside": "^5.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redbox-react": "^1.2.10",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "webpack-hot-middleware": "^2.12.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "css-mqpacker": "^5.0.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-meteor": "^3.5.2",
    "eslint-plugin-react": "^5.0.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "faker": "^3.1.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "postcss-color-hexa": "^1.2.4",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.8.1",
    "postcss-nested": "^1.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-hot-middleware": "^2.10.0"
  }
}
